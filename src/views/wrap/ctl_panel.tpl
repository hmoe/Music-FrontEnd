<div id="panel">
    <div id="min_panel">
        <div id="min_panel_down" class="pointer hmoe-icon btn-evnt" data-onclick="panel_hide">R</div>
        <div id="min_panel_up" class="pointer hmoe-icon btn-evnt" data-onclick="panel_show">U</div>
    </div>
    <div id="progress_container" class="pointer">
        <div id="progress">
            <div id="progress_loading"></div>
            <div id="progress_now">
                <p id="progress_now_text">01:02</p>
            </div>
            <div id="progress_slider" class="drag"></div>
            <p id="progress_text">03:04</p>
            <p id="progress_loading_text">
            <span class="hmoe-icon animate-text" data-content="uvwxyz&#123;|">u</span>Loading...</p>
        </div>
    </div>
    <ul id="control">
        <li class="pointer button blue hmoe-icon btn-evnt" id="ctrl_repeat" data-onclick="repeat">W</li>
        <li class="pointer button blue hmoe-icon btn-evnt" id="ctrl_previous" data-onclick="previous">c</li>
        <li class="pointer button green hmoe-icon btn-evnt" id="ctrl_pause" data-onclick="pause">Z</li>
        <li class="pointer button green hmoe-icon btn-evnt" id="ctrl_play" data-onclick="play">X</li>
        <li class="pointer button blue hmoe-icon btn-evnt" id="ctrl_next" data-onclick="next">a</li>
        <li class="pointer button blue hmoe-icon btn-evnt" id="ctrl_random" data-onclick="random">V</li>
    </ul>
    <div id="volume_container" class="pointer">
        <div id="volume">
            <div id="volume_now">
                <div id="volume_slider" class="drag hmoe-icon"></div>
            </div>
        </div>
        <span id="volume_text" class="normal hmoe-icon">Q</span>
    </div>
</div>

